{
  "name": "vue-radio-checkbox-z",
  "version": "1.0.0",
  "description": "Vue UI Check/Radio [RadioGroup, Checkbox(s)], (My-test)",
  "main": "dist/index.esm.js",
  "browser": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.esm.js",
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "clean": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "dev": "cd \"./examples\" && \"./node_modules/.bin/webpack\"",
    "start": "cd \"./examples\" && \"./node_modules/.bin/webpack-dev-server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delpikye-v/vue-radio-checkbox.git"
  },
  "homepage": "https://github.com/delpikye-v/vue-radio-checkbox",
  "author": "Delpi.Kye",
  "keywords": [
    "vue",
    "vue-radio",
    "ui-radio",
    "ui-checkbox",
    "vue-radio-group",
    "Vue-checkbox",
    "custom-radio",
    "custom-checkbox"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/delpikye-v/vue-radio-checkbox/issues"
   },
